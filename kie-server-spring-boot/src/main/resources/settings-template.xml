<?xml version="1.0"?>
<settings>
	<servers>
		<server>
			<id>remote-repo</id>
			<username>remoterepo.user</username>
			<password>remoterepo.pass</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>kie-repos</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>maven-PAM</id>
					<url>http://nexus-nexus.apps.florida-696e.openshiftworkshop.com/repository/maven-PAM/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>remote-repo</id>
					<url>remoterepo.url</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
</settings>
